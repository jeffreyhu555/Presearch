{
	"name": "Bookmarking Test",
	"author": "Furqan and Kincaid",
	"description": "The bookmark app for all your needs",
	"version": "0.1",
	"manifest_version": 2,
	"browser_action": {
		"default_icon": "icon-48.png",
		"default_popup": "popup.html"
	},
	"icons": {
		"48": "icon-48.png",
		"128": "icon-128.png"
	},
	"permissions": [
		"storage"
	],
	"content_scripts": [
		{
		"matches":["<all_urls>"],
		"js":["simple.js"]
		}]
}
